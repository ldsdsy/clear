#include<stdio.h>
int main()
{
	int i,j,k;
	char a[3][3]={'w','w','w','w','w','w','w','w','w'};
	for(k=1;k<=9;k++)
	{
		scanf("%d %d",&i,&j);
		scanf("%c",&a[i][j]);
		if(a[0][0]==a[0][1]&&a[0][1]==a[0][2]&&a[0][2]!='w'||
		   a[1][0]==a[1][1]&&a[1][1]==a[1][2]&&a[1][2]!='w'||
		   a[2][0]==a[2][1]&&a[2][1]==a[2][2]&&a[2][2]!='w'||
		   a[0][0]==a[1][1]&&a[1][1]==a[2][2]&&a[2][2]!='w'||
		   a[0][2]==a[1][1]&&a[1][1]==a[2][0]&&a[2][0]!='w'||
		   a[0][0]==a[1][0]&&a[1][0]==a[2][0]&&a[2][0]!='w'||
		   a[0][1]==a[1][1]&&a[1][1]==a[2][1]&&a[2][1]!='w'||
		   a[0][2]==a[1][2]&&a[1][2]==a[2][2]&&a[2][2]!='w')
        break;
        if(k==9)
		k++;
      
	}
	if(k==11)
		printf("No winner! Play again.\n");
	else if(a[i][j]=='#')
	printf("# is WIN!\n");
	else
		printf("* is WIN!\n");
	return 0;
}
          
